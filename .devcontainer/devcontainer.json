{
    "name": "Spring Boot Dev Container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "features": {
      "docker-in-docker": {
        "version": "latest"
      }
    },
    "postCreateCommand": "mvn clean install",
    "customizations": {
      "vscode": {
        "extensions": [
          "vscjava.vscode-java-pack",
          "pivotal.vscode-spring-boot"
        ]
      }
    }
  }
  